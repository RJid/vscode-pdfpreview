{
	"name": "pdfpreview",
	"displayName": "PDF Preview",
	"description": "Open PDF in Preview",
	"version": "1.0.0",
	"publisher": "corentinartaud",
	"engines": {
		"vscode": "^1.48.0"
	},
	"categories": [
		"Programming Languages"
	],
	"keywords": [
		"pdf",
		"pdf preview"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/corentinartaud/vscode-pdfpreview.git"
	},
	"bugs": {
		"url": "https://github.com/corentinartaud/vscode-pdfpreview/issues"
	},
	"icon": "vscode-pdfpreview.png",
	"activationEvents": [
		"onCustomEditor:pdf.preview"
	],
	"main": "./out/extension.js",
	"contributes": { 
		"customEditors": [{
			"viewType": "pdf.preview",
			"displayName": "PDF Preview",
			"selector": [{
				"filenamePattern": "*.pdf"
			}]
		}]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"package": "npx vsce package",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.48.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.0",
		"@types/node": "^14.0.27",
		"eslint": "^7.6.0",
		"@typescript-eslint/eslint-plugin": "^3.8.0",
		"@typescript-eslint/parser": "^3.8.0",
		"glob": "^7.1.6",
		"mocha": "^8.4.0",
		"typescript": "^4.0.2",
		"vscode-test": "^1.4.0"
	},
	"dependencies": {
		"open": "^7.2.1"
	}
}
